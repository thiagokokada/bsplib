#line 1901 "configure"
#include "confdefs.h"

#include <sys/types.h>
#include <sys/socket.h>
#include <stdio.h>

void main() {
  int new_sock, status;
  int optval, optlen;

  status=1;
  optval = 1542;

  new_sock = socket(AF_INET,SOCK_STREAM,0);
  if (new_sock>=0) {
    if (!setsockopt(new_sock,SOL_SOCKET,SO_SNDLOWAT,
                    (char *)&optval,sizeof(int))) {
       optlen=sizeof(optval);
       if (!getsockopt(new_sock,SOL_SOCKET,SO_SNDLOWAT,
                       (char *)&optval,&optlen)) {
         status = !(optlen && (optval == 1542));
       }
    }
  }
  exit(status);
}


EOF
