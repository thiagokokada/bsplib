#line 1972 "configure"
#include "confdefs.h"

#include <stdio.h>
#include <sys/time.h>
#include <unistd.h>

#define VERBOSE                 0
#define OVERSAMPLE            100
#define DEFAULT_RESOLUTION  25000 /* 25 Milliseconds */

void main() {
  int i, j,delta,sleep_resolution,deta,found,outbound;
  struct timeval select_tv, start_tv, end_tv;
  FILE *fptr;

  fptr=fopen("selecttest.out","w");
  if (fptr==NULL) exit(1);

  sleep_resolution = DEFAULT_RESOLUTION;
  for(i=100;i<DEFAULT_RESOLUTION;i+=500) {
    found =1;
    for(j=0;j<OVERSAMPLE;j++) {
       gettimeofday(&start_tv,NULL);
#ifdef LIBC_HAS_USLEEP
       usleep(i);
#else
       select_tv.tv_usec = i;
       select_tv.tv_sec  = 0;
       select(0,NULL,NULL,NULL,&select_tv);
#endif
       gettimeofday(&end_tv,NULL);
       delta = (end_tv.tv_usec - start_tv.tv_usec) +
